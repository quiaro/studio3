{
    "name": "studio-ui",
    "version": "0.1.0",
    "dependencies": {
        "angular": "~1.0.8",
        "angular-bootstrap": "~0.6.0",
        "angular-cookies": "~1.0.0",
        "angular-ui-router": "~0.2.5",
        "bootstrap": "3.0.0",
        "jquery": "2.0.3",
        "toastr": "1.3.1",
        "requirejs": "2.1.8",
        "require-css": "0.0.8",
        "require-less": "0.1.1",
        "requirejs-text": "2.0.10",
        "requirejs-domready": "2.0.1",
        "pubsub-js": "1.4.0",
        "studio-js-services": "0.1.0"
    },
    "devDependencies": {
        "angular-mocks": "~1.0.5",
        "angular-scenario": "~1.0.5"
    },
    "exportsOverride": {
        "angular": {
            "js": "*.js"
        },
        "angular-ui-router": {
            "js": "./release/*.js"
        },
        "angular-scenario": {
            "js": "angular-scenario.js"
        },
        "angular-*": {
            "js": "*.js"
        },
        "bootstrap": {
            "css": "./dist/css/*.css",
            "fonts": "./dist/fonts/*"
        },
        "jquery": {
            "js": "*.{js,map}"
        },
        "toastr": {
            "js": "*.{js,map}",
            "css": "*.css"
        },
        "require*": {
            "js": "*.js"
        },
        "studio-js-services": {
            "js": "./dist/*.{js,map}"
        }
    }
}
